{
  "name": "henkel-uz",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/configs/mikro-orm.config.ts",
      "./dist/configs/mikro-orm.config.js"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "/vue/**"
    ]
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build&&yarn run vite",
    "vite": "cd vue&&yarn run build",
    "start:dev": "SET NODE_ENV=development&&nodemon src/main.ts",
    "start:prod": "node dist/main",
    "serve": "vue-cli-service serve src/main.js --prefix /vue/",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "schema:initial": "mikro-orm schema:create --run",
    "schema:seeder": "mikro-orm seeder:run",
    "schema:update": "yarn mikro-orm schema:update --run",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d --build --force-recreate"
  },
  "dependencies": {
    "@grammyjs/i18n": "^0.5.1",
    "@grammyjs/menu": "^1.1.1",
    "@grammyjs/router": "1.1.1",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.0.1",
    "@nestjs/core": "^8.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^8.0.0",
    "grammy": "^1.8.2",
    "joi": "^17.9.1",
    "nestjs-pino": "^3.1.1",
    "pino-http": "^8.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "cache-loader": "^4.1.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.3.5"
  }
}
